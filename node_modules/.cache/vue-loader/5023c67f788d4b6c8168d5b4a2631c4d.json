CA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"index.vue","sourceRoot":"src/pages/Search","sourcesContent":["<template>\r\n  <div>\r\n    <typeNav />\r\n    <div class=\"main\">\r\n      <div class=\"py-container\">\r\n        <!--bread面包屑-->\r\n        <div class=\"bread\">\r\n          <ul class=\"fl sui-breadcrumb\">\r\n            <li>\r\n              <a href=\"#\">全部结果</a>\r\n            </li>\r\n          </ul>\r\n          <ul class=\"fl sui-tag\">\r\n            <!-- 分类面包屑 -->\r\n            <li class=\"with-x\" v-if=\"searchParams.categoryName\">\r\n              {{ searchParams.categoryName }}\r\n              <i @click=\"removeCategoryName\">x</i>\r\n            </li>\r\n            <!-- 关键字的面包屑 -->\r\n            <li class=\"with-x\" v-if=\"searchParams.keyword\">\r\n              {{ searchParams.keyword }}\r\n              <i @click=\"removeKeyword\">x</i>\r\n            </li>\r\n            <!-- 品牌的面包屑 -->\r\n            <li class=\"with-x\" v-if=\"searchParams.trademark\">\r\n              {{ searchParams.trademark.split(\":\")[1] }}\r\n              <i @click=\"removeTrademark\">x</i>\r\n            </li>\r\n            <!-- 平台售卖展示 -->\r\n            <li\r\n              v-for=\"(attrValue, index) in searchParams.props\"\r\n              :key=\"index\"\r\n              class=\"with-x\"\r\n            >\r\n              {{ attrValue.split(\":\")[1] }}\r\n              <i @click=\"removeAttrValue(index)\">x</i>\r\n            </li>\r\n          </ul>\r\n        </div>\r\n\r\n        <!--selector-->\r\n        <SearchSelector @trademarkInfo=\"trademarkInfo\" @attrInfo=\"attrInfo\" />\r\n\r\n        <!--details-->\r\n        <div class=\"details clearfix\">\r\n          <div class=\"sui-navbar\">\r\n            <div class=\"navbar-inner filter\">\r\n              <!-- 排序的结构 -->\r\n              <ul class=\"sui-nav\">\r\n                <li :class=\"{ active: isOne }\" @click=\"changeOrder('1')\">\r\n                  <a>\r\n                    综合<span\r\n                      v-if=\"isOne\"\r\n                      class=\"iconfont\"\r\n                      :class=\"{ 'icon-UP': isAsc, 'icon-DOWN': isDesc }\"\r\n                    ></span\r\n                  ></a>\r\n                </li>\r\n                <li :class=\"{ active: isTwo }\" @click=\"changeOrder('2')\">\r\n                  <a>\r\n                    价格\r\n                    <span\r\n                      v-if=\"isTwo\"\r\n                      class=\"iconfont\"\r\n                      :class=\"{ 'icon-UP': isAsc, 'icon-DOWN': isDesc }\"\r\n                    ></span\r\n                  ></a>\r\n                </li>\r\n                <!-- <li>\r\n                  <a >价格⬇</a>\r\n                </li> -->\r\n              </ul>\r\n            </div>\r\n          </div>\r\n          <!-- 销售的产品列表 -->\r\n          <div class=\"goods-list\">\r\n            <ul class=\"yui3-g\">\r\n              <li class=\"yui3-u-1-5\" v-for=\"good in goodsList\" :key=\"good.id\">\r\n                <div class=\"list-wrap\">\r\n                  <div class=\"p-img\">\r\n                    <!-- 在路由跳转时切记带id params参数 -->\r\n                    <router-link :to=\"`/detail/${good.id}`\"\r\n                      ><img v-lazy=\"good.defaultImg\" \r\n                    /></router-link>\r\n                  </div>\r\n                  <div class=\"price\">\r\n                    <strong>\r\n                      <em>¥</em>\r\n                      <i>{{ good.price }}.00</i>\r\n                    </strong>\r\n                  </div>\r\n                  <div class=\"attr\">\r\n                    <a\r\n                      target=\"_blank\"\r\n                      href=\"item.html\"\r\n                      title=\"促销信息，下单即赠送三个月CIBN视频会员卡！【小米电视新品4A 58 火爆预约中】\"\r\n                      >Apple苹果iPhone {{ good.title }}\r\n                    </a>\r\n                  </div>\r\n                  <div class=\"commit\">\r\n                    <i class=\"command\">已有<span>2000</span>人评价</i>\r\n                  </div>\r\n                  <div class=\"operate\">\r\n                    <a\r\n                      href=\"success-cart.html\"\r\n                      target=\"_blank\"\r\n                      class=\"sui-btn btn-bordered btn-danger\"\r\n                      >加入购物车</a\r\n                    >\r\n                    <a href=\"javascript:void(0);\" class=\"sui-btn btn-bordered\"\r\n                      >收藏</a\r\n                    >\r\n                  </div>\r\n                </div>\r\n              </li>\r\n            </ul>\r\n          </div>\r\n          <!-- 分页器 -->\r\n          <Pagination\r\n            :pageNo=\"searchParams.pageNo\"\r\n            :pageSize=\"searchParams.pageSize\"\r\n            :total=\"total\"\r\n            continues=5\r\n            @getPageNo=\"getPageNo\"\r\n          />\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport SearchSelector from \"@/pages/Search/SearchSelector/SearchSelector.vue\";\r\nimport { mapGetters, mapState } from \"vuex\";\r\nimport Pagination from \"../../components/Pagination/index.vue\";\r\n\r\nexport default {\r\n  name: \"Search\",\r\n  data() {\r\n    return {\r\n      //带给服务器的参数\r\n      searchParams: {\r\n        //一级分类的ID\r\n        category1Id: \"\",\r\n        category2Id: \"\",\r\n        category3Id: \"\",\r\n        //分类名字\r\n        categoryName: \"\",\r\n        //关键字\r\n        keyword: \"\",\r\n        //排序:初始状态应该是综合降序\r\n        order: \"1:desc\",\r\n        //分页器\r\n        pageNo: 1,\r\n        //每一页展示数据的个数\r\n        pageSize: 5,\r\n        //平台售卖属性的参数手机内存...\r\n        props: [\"\"],\r\n        //品牌\r\n        trademark: \"\",\r\n      },\r\n    };\r\n  },\r\n  beforeMount() {\r\n    // this.searchParams.category1Id = this.$route.query.category1Id\r\n    // this.searchParams.category2Id = this.$route.query.category2Id\r\n    // this.searchParams.category3Id = this.$route.query.category3Id\r\n    // this.searchParams.categoryName = this.$route.query.categoryName\r\n    // this.searchParams.keyword=this.$route.params.keyword\r\n    Object.assign(this.searchParams, this.$route.query, this.$route.params);\r\n  },\r\n  components: {\r\n    SearchSelector,\r\n    Pagination,\r\n  },\r\n  mounted() {\r\n    //在发请求之前:整理用户搜索的参数\r\n    //组件挂载完毕发一次请求\r\n    this.getData();\r\n  },\r\n  computed: {\r\n    ...mapGetters([\"goodsList\"]),\r\n    isOne() {\r\n      return this.searchParams.order.indexOf(\"1\") != -1;\r\n    },\r\n    isTwo() {\r\n      return this.searchParams.order.indexOf(\"2\") != -1;\r\n    },\r\n    isAsc() {\r\n      return this.searchParams.order.indexOf(\"asc\") != -1;\r\n    },\r\n    isDesc() {\r\n      return this.searchParams.order.indexOf(\"desc\") != -1;\r\n    },\r\n    //获取Search模块展示产品一共有多少数据\r\n    ...mapState({\r\n      total: (state) => state.search.searchList.total,\r\n    }),\r\n  },\r\n  methods: {\r\n    getData() {\r\n      //把这次请求封装成函数\r\n      this.$store.dispatch(\"getSearchList\", this.searchParams);\r\n    },\r\n    removeCategoryName() {\r\n      //删除名字\r\n      //undefined不会将字符带给服务器\r\n      this.searchParams.categoryName = undefined;\r\n      this.searchParams.category1Id = undefined;\r\n      this.searchParams.category2Id = undefined;\r\n      this.searchParams.category3Id = undefined;\r\n      //修改地址栏\r\n      if (this.$route.params) {\r\n        this.$router.push({ name: \"search\", params: this.$route.params });\r\n      }\r\n      this.getData();\r\n    },\r\n    removeKeyword() {\r\n      //给服务器\r\n      this.searchParams.keyword = undefined;\r\n      this.getData();\r\n      //通知Header清除关键字\r\n      this.$bus.$emit(\"clear\");\r\n      //路由的跳转\r\n      if (this.$route.query) {\r\n        this.$router.push({ name: \"search\", query: this.$route.query });\r\n      }\r\n    },\r\n    trademarkInfo(trademark) {\r\n      this.searchParams.trademark = `${trademark.tmId}:${trademark.tmName}`;\r\n      this.getData();\r\n    },\r\n    //删除品牌信息\r\n    removeTrademark() {\r\n      //将品牌信息置空\r\n      this.searchParams.trademark = undefined;\r\n      this.getData();\r\n    },\r\n    //收集平台属性地方 分回调\r\n    attrInfo(attr, attrValue) {\r\n      let props = `${attr.attrId}:${attrValue}:${attr.attrName}`;\r\n      //数据去除重复\r\n      if (this.searchParams.props.indexOf(props) == -1)\r\n        this.searchParams.props.push(props);\r\n      this.getData();\r\n    },\r\n    removeAttrValue(index) {\r\n      this.searchParams.props.splice(index, 1);\r\n      this.getData();\r\n    },\r\n    //点击排序\r\n    changeOrder(flag) {\r\n      let orignOeder = this.searchParams.order;\r\n      let originFlag = this.searchParams.order.split(\":\")[0];\r\n      let originSort = this.searchParams.order.split(\":\")[1];\r\n      let newOrder = \"\";\r\n      //准备一个新的order属性值\r\n      if (flag == originFlag) {\r\n        newOrder = `${originFlag}:${originSort == \"desc\" ? \"asc\" : \"desc\"}`;\r\n      } else {\r\n        //点击的价格\r\n        newOrder = `${flag}:${\"desc\"}`;\r\n      }\r\n      //将新的order赋给searchParams\r\n      this.searchParams.order = newOrder;\r\n      this.getData();\r\n    },\r\n    getPageNo(pageNo) {\r\n      //整理带给服务器参数\r\n      this.searchParams.pageNo = pageNo;\r\n      //再次发起请求\r\n      this.getData();\r\n    },\r\n  },\r\n  watch: {\r\n    //监听路由的信息\r\n    $route(newValue, oldValue) {\r\n      //再次请求发给服务器的参数\r\n      Object.assign(this.searchParams, this.$route.query, this.$route.params);\r\n      console.log(this.searchParams);\r\n      //每一次请求完毕，应该把相应的1.2.3级分类的id置空\r\n      this.searchParams.category1Id = undefined;\r\n      this.searchParams.category2Id = undefined;\r\n      this.searchParams.category3Id = undefined;\r\n      this.getData();\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.main {\r\n  margin: 10px 0;\r\n\r\n  .py-container {\r\n    width: 1200px;\r\n    marg